{
  "name": "admall",
  "version": "3.0.0",
  "scripts": {
    "start": "vue-cli-service serve --model local --s",
    "build": "vue-cli-service build --model production && npm run sentry",
    "build:dev": "vue-cli-service build --model development",
    "sentry": "node ./sentry.js --model production",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "ali-oss": "~6.2.1",
    "axios": "~0.19.0",
    "compressorjs": "~1.0.6",
    "echarts": "~4.5.0",
    "element-theme-chalk": "~2.12.0",
    "element-ui": "~2.13.0",
    "moment": "~2.24.0",
    "nprogress": "0.2.0",
    "qiankun": "^2.0.16",
    "qs": "~6.9.1",
    "quill": "~1.3.7",
    "v-charts": "~1.19.0",
    "v-viewer": "~1.4.2",
    "vue": "~2.6.10",
    "vue-awesome-swiper": "~3.1.3",
    "vue-clipboard2": "~0.3.1",
    "vue-router": "~3.1.3",
    "vuex": "~3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~7.6.1",
    "@commitlint/config-conventional": "~7.6.0",
    "@vue/cli-plugin-babel": "3.12.1",
    "@vue/cli-plugin-eslint": "3.12.1",
    "@vue/cli-service": "3.12.1",
    "@vue/eslint-config-standard": "~4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "~10.0.3",
    "babel-jest": "~24.9.0",
    "babel-plugin-component": "~1.1.1",
    "babel-preset-es2015": "~6.24.1",
    "eslint": "~5.8.0",
    "eslint-plugin-vue": "~5.2.3",
    "husky": "~1.2.0",
    "sass": "~1.26.5",
    "sass-loader": "~7.3.1",
    "vue-loader": "~15.7.2",
    "vue-template-compiler": "~2.6.10"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "eslint --ext .js,.vue src",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
